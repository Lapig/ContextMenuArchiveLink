{
  "manifest_version": 2,

  "name": "pigtest123",
  "description": "TEsting",
  "version": "1.0",
  "content_scripts": [
    {
      "js": [
        "script.js"
      ],
      "matches": [
        "*://*/*"
      ],
      "run_at": "document_end"
    }
  ],
  "icons": {
    "16": "icon16.png",
    "128": "icon128.png"
  },
  "browser_action": {
    "default_icon": {
      "19": "icon16.png",
      "38": "icon.png"
    },
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "<all_urls>"
  ]
}